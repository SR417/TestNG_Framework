package ConditionBlocks;

public class IfElse {
	
	public static void main(String[] args) {
	
		int a=10;
		if (a<12) {
			System.out.println("Hello");
		}
		else {
			System.out.println("Hi");
		}
		
		int b=12;
		if (b<12) {
			System.out.println("Hello");
		}
		else if (b<10){
			System.out.println("Hi");
		}
			
			else {
				System.out.println("No one is matching");
			}
				
		}
	}
	

